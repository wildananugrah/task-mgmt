{
  "name": "76-api-generic",
  "module": "src/server.ts",
  "type": "module",
  "private": true,
  "scripts": {
    "dev": "bun run --watch src/server.ts",
    "start": "bun run src/server.ts",
    "db:generate": "bunx prisma generate",
    "db:migrate": "bunx prisma migrate dev",
    "db:push": "bunx prisma db push",
    "db:seed": "bun run prisma/seed.ts",
    "db:studio": "bunx prisma studio",
    "db:reset": "bunx prisma migrate reset",
    "build": "bun build src/server.ts --target=bun --outdir=dist",
    "docker:up": "docker-compose up -d",
    "docker:down": "docker-compose down",
    "docker:logs": "docker-compose logs -f",
    "test": "bun test"
  },
  "devDependencies": {
    "@types/bcrypt": "^6.0.0",
    "@types/bun": "latest",
    "@types/cors": "^2.8.19",
    "@types/jsonwebtoken": "^9.0.10",
    "@types/pg": "^8.15.6",
    "@types/uuid": "^9.0.8"
  },
  "peerDependencies": {
    "typescript": "^5"
  },
  "dependencies": {
    "@apollo/server": "^5.2.0",
    "@aws-sdk/client-s3": "^3.940.0",
    "@prisma/adapter-pg": "^7.0.1",
    "@prisma/client": "^7.0.1",
    "@scalar/api-reference": "^1.40.0",
    "bcrypt": "^6.0.0",
    "cors": "^2.8.5",
    "express-rate-limit": "^8.2.1",
    "graphql": "^16.12.0",
    "graphql-tag": "^2.12.6",
    "jsonwebtoken": "^9.0.2",
    "minio": "^7.1.3",
    "multer": "^1.4.5-lts.1",
    "pg": "^8.16.3",
    "prisma": "^7.0.1",
    "swagger-ui-dist": "^5.30.3",
    "uuid": "^9.0.1",
    "winston": "^3.11.0",
    "winston-loki": "^6.0.8",
    "zod": "^4.1.13"
  }
